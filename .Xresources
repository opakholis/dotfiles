! URXVT APPEARANCE
URxvt.font:                       xft:JetBrainsMono Nerd Font Mono:size=10
URxvt.geometry:                   85x20
URxvt.internalBorder:             20
URxvt.cursorBlink:                true
URxvt.cursorUnderline:            true
URxvt.saveline:                   32768
URxvt.scrollBar:                  false
URxvt.scrollBar_right:            true
URxvt.scrollBar_floating:         false
URxvt.scrollstyle:                plain
URxvt.urgentOnBell:               false
URxvt.depth:                      24
URxvt.iso14755:                   false

! URXVT EXTENSIONS
URxvt.perl-ext-common:            default,clipboard,keyboard-select,url-select,tabbedex

URxvt.clipboard.copycmd:          xclip -i -selection clipboard
URxvt.clipboard.pastecmd:         xclip -o -selection clipboard
URxvt.keysym.M-c:                 perl:clipboard:copy
URxvt.keysym.M-v:                 perl:clipboard:paste
URxvt.keysym.M-Escape:            perl:keyboard-select:activate
URxvt.keysym.M-s:                 perl:keyboard-select:search

URxvt.urlLauncher:                firefox
URxvt.underlineURLs:              true
URxvt.urlButton:                  1

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

! Rofi
! rofi.font: monospace 12
! rofi.padding: 20
! rofi.line-padding: 2
! rofi.color-enabled: true
! rofi.hide-scrollbar: true
! rofi.separator-style: solid
! rofi.color-window: #1c2023, #919ba0, #1c2023
! rofi.color-normal: #1c2023, #919ba0, #1c2023, #a4a4a4, #1c2023
! rofi.color-urgent: argb:00000000, #f43753, argb:00000000, argb:00000000, #e29a49
! rofi.color-active: argb:00000000, #49bbfb, argb:00000000, argb:00000000, #e29a49
! rofi.kb-cancel: Escape,Alt+F1
